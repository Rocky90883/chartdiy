<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.springboot.mapper.hualala.BdhualalaMapper">


  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultType="com.atguigu.springboot.bean.hualala.Bdhualala">
    select *
    from bdhualala
    where id = #{id,jdbcType=VARCHAR}
  </select>

  <select id="selectByOrderSubType" resultType="com.atguigu.springboot.bean.hualala.Bdhualala" parameterType="com.atguigu.springboot.bean.hualala.Bdhualala">
    select *
    from bdhualala
    where orderStatus = #{orderStatus,jdbcType=VARCHAR}
  </select>


</mapper>